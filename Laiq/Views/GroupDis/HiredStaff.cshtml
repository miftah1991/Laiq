            { name: 'Position', type: 'string' },
            { name: 'Salary', type: 'number' },
            { name: 'JoinDate', type: 'date' },
            { name: 'PositionLevel', type: 'string' },
            { name: 'HireRemarks', type: 'string' },
            { name: 'ARID', type: 'number' },



        ];
        fillGrid(datafields, varGrid, varURL);
    }
</script>
