        $("#NProvincerow").hide();
        $("#AddressCountryID").change(function () {
            if ($("#AddressCountryID").val() == 1) {
                $("#Provincerow").show();
            } else {
                $("#Provincerow").hide();
            }
        })
        $("#NAddressCountryID").change(function () {
            if ($("#NAddressCountryID").val() == 1) {
                $("#NProvincerow").show();
            } else {
                $("#NProvincerow").hide();
            }
        })
        $('.AddEdu').click(function () {
            i = rowCounts = $('#tbody tr').length;
            addRow(i);
        });

        $("#QualificationID").change(function () {

            i = rowCounts = $('#tbody tr').length;
            $('#tbody tr').remove();
            var QualID = $("#QualificationID").val();
            if (QualID == 1) {
                addRowEdu(i, 1, '@Laiq.Resource.PhD','PhD');
                addRowEdu(Number(i) + 1, 2, '@Laiq.Resource.Master','Master');
                addRowEdu(Number(i) + 2, 3, '@Laiq.Resource.Bachelor', 'Bachelor');
            } else if (QualID == 2) {
                addRowEdu(Number(i) + 1, 2, '@Laiq.Resource.Master', 'Master');
                addRowEdu(Number(i) + 2, 3, '@Laiq.Resource.Bachelor', 'Bachelor');
            } else {
                addRowEdu(Number(i) + 2, 3, '@Laiq.Resource.Bachelor', 'Bachelor');
            }

        });
        $("#WorkExp").change(function () {
            var WorkExp = $("#WorkExp").val();
            if (WorkExp == 'Yes') {
                $("#ExpBody").show();
            } else {
                $("#ExpBody").hide();

            }
        });


        $("#ExpBody").hide();
        $('#AddLang').click(function () {
            i = rowCounts = $('#tbodyLang tr').length;
            addRowLang(i);
        });
        $('#AddExp').click(function () {
            i = rowCounts = $('#tbodyJobDetail tr').length;
