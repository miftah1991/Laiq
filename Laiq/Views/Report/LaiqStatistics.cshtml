<script src="~/Scripts/pie-chart.js"></script>
<script src="~/Scripts/JScript.js"></script>
<script type="text/javascript">
    
    $(document).ready(function () {
        drawProvinceChart()
    });
    function drawProvinceChart() {
        var eduJSON = new Array();
        var eduLabel = new Array();
        $.getJSON('/Dashboard/GetChartByLocation', function (data) {
            for (i = 0; i < data.length; i++) {

                eduJSON.push([data[i].Category, data[i].Counts]);
                eduLabel.push([data[i].Category]);

            }
            visitorData1("mainContainter", "Chart Based Applicant Location", "Applicants", "Location", eduJSON, eduLabel, -85, '/Dashboard/GetByEFData/')
        });
    }
</script>
