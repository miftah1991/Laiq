                            <option value="M">@Laiq.Resource.Male</option>
                            <option value="F">@Laiq.Resource.Female</option>
                        </select>
                    </div>
                </div>

            </div>
            <div class="col-lg-12">
                <div class="form-group row">
                    <label for="inputPassword" class="col-lg-2 col-form-label">@Laiq.Resource.Mobile</label>
